@{
    ViewBag.Title = "Home Page";
}
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <title>Google Maps Multiple Markers</title>
    <script>
        var latitudes = @Html.Raw(Json.Encode(@ViewBag.Latitudes));
        
        var longitudes = @Html.Raw(Json.Encode(@ViewBag.Longitudes));
     
        
    </script>
</head>
<body>
    <div id="map" style="width: 800px; height: 400px;"></div>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAqPB-xlRlEDxCQcWVRI0pZ9UJCHDhNzaE&callback=initMap"></script>
    <script>
        function initMap() {
            var milwaukee = {lat: 43.038, lng: -87.906}
            var map = new google.maps.Map(document.getElementById('map'), { zoom: 15, center: milwaukee });
            for (i = 0; i < 3; i++)
            {

                var coord = {lat: latitudes[i], lng: longitudes[i]};
                marker = new google.maps.Marker({
                    position: coord, map: map
                });
            }
        }
    </script>
   
